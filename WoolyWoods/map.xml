<team color="dark red" max="32">Red Team</team>
</teams>
<kits>
<kit name="spawn">
<item slot="0" damage="-3000">stone sword</item>
<item slot="2" name="Spade" damage="-3000">shears</item>
<item slot="1" enchantment="arrow infinite:1" damage="-3000">bow</item>
<item slot="3" amount="64">golden carrot</item>
<item slot="4" amount="1">golden apple</item>
<item slot="28" amount="1">arrow</item>
<potion duration="5" amplifier="4">regeneration</potion>
<potion duration="10" amplifier="4">damage resistance</potion>
</kit>
<kit name="red" parents="spawn">
<helmet color="cd0000" damage="-3000">leather helmet</helmet>
<chestplate color="cd0000" damage="-3000" enchantment="protection projectile:2">leather chestplate</chestplate>
<leggings damage="-3000">gold leggings</leggings>
<boots color="cd0000" damage="-3000" enchantment="protection fall:2">leather boots</boots>
<item slot="5" amount="16" damage="14">wool</item>
</kit>
<kit name="blue" parents="spawn">
<helmet color="0066cc" damage="-3000">leather helmet</helmet>
<chestplate color="0066cc" damage="-3000" enchantment="protection projectile:2">leather chestplate</chestplate>
<leggings damage="-3000">gold leggings</leggings>
<boots color="0066cc" damage="-3000" enchantment="protection fall:2">leather boots</boots>
<item slot="5" amount="16" damage="11">wool</item>
</kit>
</kits>
<spawns>
<spawn team="blue" kit="blue" yaw="0">
<cylinder base="72,29,-98" radius="2" height="0"/>
</spawn>
<spawn team="red" kit="red" yaw="-179">
<cylinder base="82,29,98" radius="2" height="0"/>
</spawn>
<default yaw="-90">
<cylinder base="0,66,0" radius="3" height="0"/>
</default>
</spawns>
<regions>
<apply block="deny-all" message="You may not interact with blocks outside the playing field.">
<negative>
<rectangle min="32,-107" max="123,108"/>
</negative>
</apply>
<apply block="deny-all" message="You may not place or break blocks in and around the spawn area!">
<cuboid name="red-base" min="99,28,107" max="68,41,87"/>
<cuboid name="blue-base" min="55,28,-107" max="86,41,-87"/>
</apply>
</regions>
<maxbuildheight>55</maxbuildheight>
<timelock>on</timelock>
<toolrepair>
<tool>stone sword</tool>
<tool>bow</tool>
<tool>shears</tool>
</toolrepair>
<itemremove>
<item>arrow</item>
<item>golden carrot</item>
<item>leather helmet</item>
<item>leather chestplate</item>
<item>gold leggings</item>
<item>leather boots</item>
</itemremove>
