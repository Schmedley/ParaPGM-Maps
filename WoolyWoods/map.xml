<map proto="1.3.0">
<name>Wooly Woods</name>
<version>1.1.2</version>
<objective>Be the team with the most kills after 15 minutes!</objective>
<authors>
<author>kp10708</author>
<author>JoeltheMole</author>
</authors>
<contributors>
