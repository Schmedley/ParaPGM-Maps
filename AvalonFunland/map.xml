<?xml version="1.0"?>
<map proto="1.3.0">
<name>Avalon Funland</name>
<version>1.0</version>
<objective>Destroy the enemy team's core on top of their tower</objective>
<authors>
	<author contribution="Map Design">911chandler</author>
	<author contribution="Map Design">Nadastorm</author>
</authors>
<rules>
	<rule>No Team Griefing</rule>
</rules>
<teams>
	<team color="blue" max="50">Blue Team</team>
	<team color="dark red" max="50">Red Team</team>
</teams>
<spawns>
	<spawn team="blue" yaw="0"><cylinder base="49,22,-80" radius="4" height="2"/></spawn>
	<spawn team="red" yaw="0"><cylinder base="49,22,81" radius="4" height="2"/></spawn>
 <default yaw="0"><cylinder base="0,32,0" radius="6" height="2"/></default>
</spawns>
<cores material="obsidian" leak="5">
    <core team="blue"><cuboid min="47,48,-139" max="51,52,-133"/></core>
    <core team="red"><cuboid min="-47,48,134" max="51,52,140"/></core>
</cores>
<modifybowprojectile>
	<projectile>LightningStrike</projectile>
</modifybowprojectile>

</map>