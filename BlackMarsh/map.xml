<filters>
        <filter name="blocks" parents="deny-all">
        <allow><block>blue wool</block></allow>
        <allow><block>lime wool</block></allow>
        <allow><block>TNT</block></allow>
        <allow><block>sponge</block></allow>
        </filter>
 
        <filter name="only-blue" parents="deny-players">
                <allow><team>blue</team></allow>
        </filter>
 
        <filter name="only-green" parents="deny-players">
                <allow><team>green</team></allow>
        </filter>
</filters>
 
<regions>
        <apply block="blocks" message="You may not modify the map.">
                <cuboid min="-o,-o" max="o,o"/>
        </apply>
 
        <apply enter="only-blue" message="You may not enter the enemy team's base!">
                <circle center="1769,577" radius="10"/>
        </apply>
 
        <apply enter="only-green" message="You may not enter the enemy team's base!">
                <circle center="2037,577" radius="10"/>
        </apply>
</regions>
 
<spawns>
        <spawn team="green" kit="green" yaw="90">
                <cuboid min="2034,32,579" max="2040,32,575"/>
        </spawn>
       
        <spawn team="blue" kit="blue" yaw="90">
                <cuboid min="1771,32,580" max="1767,32,574"/>
        </spawn>
 
        <default yaw="0">
                <cuboid min="1903,86,487" max="1907,86,491"/>
        </default>
</spawns>
 
<timelock>on</timelock>
 
<maxbuildheight>70</maxbuildheight>
 
<toolrepair>
        <tool>iron sword</tool>
        <tool>bow</tool>
</toolrepair>
 
<itemremove>
        <item>shears</item>
        <item>golden apple</item>
        <item>leather helmet</item>
        <item>iron chestplate</item>
        <item>leather leggins</item>
        <item>iron boots</item>
</itemremove>
 
<disabledamage>
    <damage>fall</damage>
</disabledamage>
 
<instantignite>on</instantignite>
<blockdamage>off</blockdamage>
 
