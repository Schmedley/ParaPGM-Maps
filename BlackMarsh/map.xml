<?xml version="1.0"?>
<map proto="1.3.0">
<name>Black Marsh</name>
<version>1.0</version>
<objective>Destroy all of the enemy team's 7 monuments to win!</objective>
 
<authors>
        <author>Tywnis</author>
</authors>
 
<contributors>
        <contributor contribution="XML Coding">ItsMiiOlly</contributor>
        <contributor contribution="XML Coding">MrTazz4077</contributor>
</contributors>
 
<rules>
        <rule>Don't team kill with the TNT!</rule>
</rules>
 
<teams>
        <team color="blue" max="40">Toads</team>
        <team color="green" max="40">Frogs</team>
</teams>
 
<kits>
        <kit name="blue">
                <item slot="0">iron sword</item>
                <item slot="1" enchantment="punch:2">bow</item>
                <item slot="2" enchantment="unbreaking:3, knockback:2">shears</item>
                <item slot="3" amount="4">tnt</item>
                <item slot="4" amount="8">cobweb</item>
                <item slot="5" amount="32">blue wool</item>
                <item slot="6" amount="64">arrow</item>
                <item slot="7" amount="64">arrow</item>
                <item slot="8" amount="32">golden apple</item>
                <potion duration="7" amplifier="1">heal</potion>
                <potion duration="150000" amplifier="3">jump</potion>
        </kit>
 
        <kit name="green">
                <item slot="0">iron sword</item>
                <item slot="1" enchantment="punch:2">bow</item>
                <item slot="2" enchantment="unbreaking:3, knockback:2">shears</item>
                <item slot="3" amount="4">tnt</item>
                <item slot="4" amount="8">cobweb</item>
                <item slot="5" amount="32">lime wool</item>
                <item slot="6" amount="64">arrow</item>
                <item slot="7" amount="64">arrow</item>
                <item slot="8" amount="32">golden apple</item>
                <potion duration="7" amplifier="1">heal</potion>
                <potion duration="150000" amplifier="3">jump</potion>
        </kit>
       
        <kit name="blue-armor" parents="blue">
                <helmet color="0066cc">leather helmet</helmet>
                <chestplate enchantment="blastprotection:3">iron chestplate</chestplate>
                <leggins color="0066cc">leather leggins</leggins>
                <boots enchantment="featherfalling:3">iron boots</boots>
        </kit>
 
        <kit name="green-armor" parents="green">
                <helmet color="3dbd00">leather helmet</helmet>
                <chestplate enchantment="blastprotection:3">iron chestplate</chestplate>
                <leggins color="3dbd00">leather leggins</leggins>
                <boots enchantment="featherfalling:3">iron boots</boots>
        </kit>
</kits>
 
